# Redis Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_api_key_here
SUPABASE_KEY=your-supabase-service-role-key-here

# Telegram Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_OPERATOR_CHAT_ID=your_chat_id_here

# Google Cloud Configuration
GCP_CREDENTIALS_PATH=path/to/your/credentials.json
GEMINI_API_KEY=your_gemini_api_key_here

# Speech Configuration
SPEECH_LANGUAGE_CODE=ml-IN
SPEECH_VOICE_NAME=ml-IN-Standard-A
SPEECH_SAMPLE_RATE=16000

# Call Configuration
MAX_PHONE_LENGTH=10
CALL_TIMEOUT_SECONDS=300

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> | <level>{level: <8}</level> | <cyan>{name}</cyan>:<cyan>{function}</cyan>:<cyan>{line}</cyan> - <level>{message}</level> 